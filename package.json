{
  "name": "fjl-data-core",
  "version": "1.0.0",
  "description": "Fjl's `Monad`, `Maybe` and `Either` packages.",
  "main": "dist/cjs/fjl-data-core.js",
  "module": "./dist/es6-module/fjl-data-core.js",
  "scripts": {
    "build": "gulp clean && gulp build && yarn test-builds",
    "docs": "gulp jsdoc",
    "test-builds": "jest --testMatch **/tests/*-test-*.js",
    "test": "jest --testMatch **/tests/test-*.js",
    "eslint": "eslint"
  },
  "keywords": [
    "functional",
    "functor",
    "monad",
    "monoid",
    "prototypical",
    "classical",
    "functional-jslib",
    "fjl",
    "oop",
    "oo"
  ],
  "author": "Ely De La Cruz <elycruz@elycruz.com>",
  "license": "BSD-3-Clause",
  "dependencies": {
    "fjl": "^0.21.13"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "jest": "^22.2.2"
  },
  "repository": "git@github.com:functional-jslib/fjl-data-core.git",
  "jest": {
    "testMatch": [
      "**/tests/test-*.js"
    ],
    "testEnvironment": "node"
  },
  "rollupConfig": {
    "external": [
      "fjl",
      "fjl-validator",
      "fjl-mutable"
    ]
  },
  "buildConfig": {
    "inputModuleName": "fjlDataCore",
    "inputFilePath": "./src/fjlDataCore.js",
    "outputFileName": "fjl-data-core.js",
    "outputFileNameMin": "fjl-data-core.min.js",
    "srcsGlob": "./src/**/*.js",
    "paths": {
      "dist": "./dist",
      "docs": "./docs"
    },
    "folderNames": {
      "amd": "amd",
      "cjs": "cjs",
      "iife": "iife",
      "umd": "umd",
      "es6Module": "es6-module"
    },
    "es6ModuleRollup": {
      "config": {
        "input": "./src/fjlDataCore.js",
        "external": [
          "fjl",
          "fjl-mutable"
        ]
      },
      "write": {
        "file": "./dist/es6-module/fjl-data-core.js",
        "format": "es",
        "sourcemap": true
      }
    },
    "amdRollup": {
      "babelPlugin": {
        "babelrc": false,
        "presets": [
          [
            "env",
            {
              "modules": false
            }
          ]
        ],
        "plugins": [
          "transform-es2015-modules-amd"
        ]
      }
    },
    "cjsRollup": {
      "babelPlugin": {
        "babelrc": false,
        "presets": [
          [
            "env",
            {
              "modules": false
            }
          ]
        ],
        "plugins": [
          "babel-plugin-add-module-exports"
        ]
      }
    },
    "iifeRollup": {
      "babelPlugin": {
        "babelrc": false,
        "presets": [
          [
            "env",
            {
              "modules": false
            }
          ]
        ],
        "plugins": [
          "external-helpers"
        ]
      }
    },
    "umdRollup": {
      "babelPlugin": {
        "babelrc": false,
        "presets": [
          [
            "env",
            {
              "modules": false
            }
          ]
        ],
        "plugins": [
          "babel-plugin-add-module-exports",
          "transform-es2015-modules-umd"
        ]
      }
    }
  }
}
